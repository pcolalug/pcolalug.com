{% extends "layout.jinja2" %}
{% block content %}
<h1>Upcoming Meetings:</h1>
<div id="sponsors">
  <h3>Recent Sponsors</h3>
  <ul>
    <li><a href="http://ewh.ieee.org/r3/nwflorida/" target="_blank"><img src="/static/images/ieee_logo_mb_tagline.gif" alt="IEEE Northwest Florida Section - Computer and Communications Joint Society" /></a></li>
    <li><a href="http://www.teksystems.com/it-careers/search-results?toff=1013435&officeid=1012" target="_blank"><img src="/static/images/teksystems.png" alt="TEKsystems" /></a></li>
    <li><a href="http://oreilly.com/ug/" target="_blank"><img src="/static/images/oreilly.gif" alt="O'Reilly User Group Program" /></a></li>
  </ul>
</div>
<div id="primary">
{% for event in events %}
<p>
    <div>
        <h3>{{ event.start }}</h3>
        Meeting At: {{ event.location }}
        <div>{{ event.description }}</div>
    </div>
</p>
{% endfor %}
</div>
{% endblock %}
